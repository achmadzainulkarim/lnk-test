{
    "compilerOptions": {
      "module": "CommonJS",
      "baseUrl" : "./",
      "moduleResolution": "Node",
      "outDir": "dist",
      "sourceMap": true,
      "lib": ["es5", "es6", "dom"],
      "target": "es2015",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "paths": {
        "undici-types": ["node_modules/undici-types/index.d.ts"],
        // "@/*": ["./src/*"]
      },
    },
    "include": [
      "src/**/*.ts"
    ],
    "types" : [
      "node",
      "redis"
    ]
  }
  